{
  "$schema": "https://unpkg.com/knip@5/schema-jsonc.json",
  "entry": [
    "src/cli.ts!",
    "bin/**!"
  ],
  "project": [
    "src/**!",
    "tests/**",
    "!tests/_data/dummy_projects/**",
    "!tests/_data/*-results/**"
  ],
  "ignore": [
    "tools/**"
  ],
  "ignoreDependencies": [
    // needed to force the installation of the optional dependency of a 3rd party package:
    "xmlbuilder2"
  ]
}
